@model IEnumerable<OfficeManagement.Models.ApplicationUser>
@{
    ViewData["Title"] = "Admin";
}

<h2>Admin</h2>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PicturePath)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.UserName, "UserDetail", new { userName = item.UserName })
            </td>
            <td>
                @item.PhoneNumber
            </td>
            <td>
                <img src="@item.PicturePath" class="" width="150px" height="80px"/>
            </td>
        </tr>
    }

</table>
<div>
    @Html.ActionLink("Back to List", "Login")
</div>